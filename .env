# API Configuration
VITE_API_URL=https://e-commerce-backend-production-5d11.up.railway.app
VITE_API_TIMEOUT=30000
VITE_API_VERSION=v1

# Application Configuration
VITE_APP_NAME=ECommerce Marketplace
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Multi-vendor marketplace for online shopping
VITE_APP_URL=http://localhost:3000
VITE_CONTACT_EMAIL=support@marketplace.com

# Environment
VITE_NODE_ENV=development
VITE_APP_ENV=development

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_CHAT_SUPPORT=false
VITE_ENABLE_WISHLIST=true
VITE_ENABLE_REVIEWS=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_MULTI_LANGUAGE=false
VITE_ENABLE_DARK_MODE=true
VITE_ENABLE_PWA=false

# Payment Gateways
VITE_STRIPE_PUBLISHABLE_KEY=
VITE_PAYPAL_CLIENT_ID=
VITE_RAZORPAY_KEY_ID=

# External Services
VITE_GOOGLE_ANALYTICS_ID=
VITE_GOOGLE_MAPS_API_KEY=
VITE_CLOUDINARY_CLOUD_NAME=
VITE_CLOUDINARY_API_KEY=
VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=

# Social Media Integration
VITE_FACEBOOK_APP_ID=
VITE_GOOGLE_CLIENT_ID=
VITE_TWITTER_API_KEY=

# File Upload
VITE_MAX_FILE_SIZE=5242880
VITE_ALLOWED_IMAGE_TYPES=image/jpeg,image/png,image/webp
VITE_CLOUDINARY_UPLOAD_PRESET=

# Pagination & Limits
VITE_PRODUCTS_PER_PAGE=12
VITE_SEARCH_RESULTS_LIMIT=20
VITE_REVIEW_CHAR_LIMIT=500

# Security
VITE_ENABLE_RECAPTCHA=false
VITE_RECAPTCHA_SITE_KEY=

# Vendor Features
VITE_ENABLE_VENDOR_REGISTRATION=true
VITE_VENDOR_COMMISSION_RATE=10
VITE_MIN_PAYOUT_AMOUNT=50

# Cache & Performance
VITE_ENABLE_SERVICE_WORKER=false
VITE_CACHE_DURATION=3600000

# Shipping
VITE_DEFAULT_CURRENCY=USD
VITE_CURRENCY_SYMBOL=$
VITE_SHIPPING_CALCULATOR_API=

# Notification Services
VITE_PUSHER_APP_KEY=
VITE_PUSHER_CLUSTER=
VITE_EMAIL_SERVICE_API=

# Development Tools
VITE_ENABLE_REDUX_DEVTOOLS=true
VITE_ENABLE_DEBUG_MODE=true
VITE_LOG_LEVEL=info
